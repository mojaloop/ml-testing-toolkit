  {
    "ruleId": 21,
    "priority": 1,
    "description": "post /transfers testingtoolkitdfsp",
    "apiVersion": {
      "minorVersion": 0,
      "majorVersion": 1,
      "type": "fspiop",
      "asynchronous": true
    },
    "conditions": {
      "all": [
        {
          "fact": "body",
          "operator": "equal",
          "value": "testingtoolkitdfsp",
          "path": "payerFsp"
        },
        {
          "fact": "operationPath",
          "operator": "equal",
          "value": "/transfers"
        },
        {
          "fact": "method",
          "operator": "equal",
          "value": "post"
        }
      ]
    },
    "event": {
      "method": "put",
      "path": "/transfers/{ID}",
      "params": {
        "scripts": {
          "enabled": true,
          "exec": [
            "if (pm.environment.get('payerFspId') === '27713803912') {",
            "  var ttkdfspTotalAmount = pm.environment.get('ttkdfspTotalAmount')",
            "  if (!ttkdfspTotalAmount) {",
            "    ttkdfspTotalAmount = 0",
            "  }",
            "  const body = JSON.parse(pm.request.body)",
            "  ttkdfspTotalAmount += +body.amount.amount",
            "  pm.environment.set('ttkdfspTotalAmount', ttkdfspTotalAmount)",
            "}"
          ]
        }
      },
      "type": "MOCK_CALLBACK"
    },
    "type": "callback",
    "version": 1
  },
