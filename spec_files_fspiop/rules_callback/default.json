[
  {
    "ruleId": 1,
    "priority": 1,
    "description": "Send a mock callback for all the transfer requests",
    "conditions": {
      "all": [
        {
          "fact": "operationPath",
          "operator": "equal",
          "value": "/transfers"
        },
        {
          "fact": "method",
          "operator": "equal",
          "value": "post"
        }
      ]
    },
    "event": {
      "type": "MOCK_CALLBACK",
      "params": {},
      "method": "put",
      "path": "/transfers/{ID}"
    }
  },
  {
    "ruleId": 2,
    "description": "Send a mock callback for all the parties lookup with a constant party info",
    "conditions": {
      "all": [
        {
          "fact": "operationPath",
          "operator": "equal",
          "value": "/parties/{Type}/{ID}"
        },
        {
          "fact": "method",
          "operator": "equal",
          "value": "get"
        }
      ]
    },
    "event": {
      "type": "MOCK_CALLBACK",
      "params": {
      },
      "method": "put",
      "path": "/parties/{Type}/{ID}"
    }
  },
  {
    "ruleId": 3,
    "description": "Send a mock callback for all quotes requests",
    "conditions": {
      "all": [
        {
          "fact": "operationPath",
          "operator": "equal",
          "value": "/quotes"
        },
        {
          "fact": "method",
          "operator": "equal",
          "value": "post"
        }
      ]
    },
    "event": {
      "type": "MOCK_CALLBACK",
      "params": {
        "headers": {},
        "body": {
          "payeeFspCommission": {
            "amount": "20"
          }
        }
      },
      "method": "put",
      "path": "/quotes/{ID}"
    }
  }
]
